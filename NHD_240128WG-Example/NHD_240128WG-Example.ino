/*****************************************************
*                 Function Commands                  *
*****************************************************/

void comm_out(unsigned char c)
{
  digitalWrite(RS, HIGH);      //C/D = H = command
  PORTA = c;
  digitalWrite(WR, LOW);
  digitalWrite(WR, HIGH);
}


void data_out(unsigned char d)
{
  digitalWrite(RS, LOW);     //C/D = L = data
  PORTA = d;
  digitalWrite(WR, LOW);
  digitalWrite(WR, HIGH);
}


void dispFill()
{
	unsigned int i, j;
        comm_out(0x98);            //graphics ON, text OFF
        data_out(0x00);            
        data_out(0x40);
        comm_out(0x24);            //set address pointer = 0x0040 (first pixel)
        
        comm_out(0xB0);            //auto write ON
        for(i=0;i<30720;i++)    //fill screen with pixels
        {
          data_out(0xFF);
        }
        comm_out(0xB2);        //auto write OFF
}

void dispClear()
{
  unsigned int i, j;
        comm_out(0x98);            //graphics ON, text OFF
        data_out(0x00);            
        data_out(0x40);
        comm_out(0x24);            //set address pointer = 0x0040 (first pixel)
        
        comm_out(0xB0);            //auto write ON
        for(i=0;i<30720;i++)    //fill screen with pixels
        {
          data_out(0x00);
        }
        comm_out(0xB2);        //auto write OFF
}

void dispImage(unsigned char *lcd_string)
{
  unsigned int i, j;
        comm_out(0x98);            //graphics ON, text OFF
        data_out(0x00);            
        data_out(0x40);
        comm_out(0x24);            //set address pointer = 0x0040 (first pixel)
        
        comm_out(0xB0);            //auto write ON
        for(i=0;i<7680;i++)    //fill screen with pixels
        {
          data_out(*lcd_string);
          lcd_string++;
        }
        comm_out(0xB2);        //auto write OFF
}

/****************************************************
* Initialization For controller + Setup, to run once*
*****************************************************/

void setup()
{
  DDRC = 0xFF;         //set PORTC (control signals) as output
  PORTC = 0x00;
  DDRA = 0xFF;         //set PORTA (data signals) as output
  PORTA = 0x00;
  digitalWrite(RD, HIGH);
  digitalWrite(WR, HIGH);
  digitalWrite(RES, HIGH);
  
  comm_out(0x40);      //text home address
  data_out(0x00);
  data_out(0x40);

  comm_out(0x42);      //graphic home address
  data_out(0x1E);
  data_out(0x00);  
  
  comm_out(0x41);      //text area
  data_out(0x1E);
  data_out(0x00);  
  
  comm_out(0x43);      //graphic area
  
  comm_out(0x80);      //OR mode     
  
  delay(10);
}

/*****************************************************
*           Loop Function, to run repeatedly         *
*****************************************************/

void loop()
{ 
  dispFill();
  delay(1000);
  dispClear();
  delay(1000);

  dispImage(NHD_Logo);
  delay(1500);
  dispClear();
  delay(1000);
}
